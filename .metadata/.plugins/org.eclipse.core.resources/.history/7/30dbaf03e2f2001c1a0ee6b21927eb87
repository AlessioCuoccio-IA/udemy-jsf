package com.luv2code.jsf.beandemo;

import javax.faces.bean.ApplicationScoped;
import javax.faces.bean.ManagedBean;

@ManagedBean
@ApplicationScoped
public class CounterTwo {
	
	private Integer value = 0;
	
	public CounterTwo() {}
	
	public String increment() {
		value++;
		
		return "counter_two";
	}

}
